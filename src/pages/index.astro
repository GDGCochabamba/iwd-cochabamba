---
import Layout from "../layouts/Layout.astro";
import Menu from "../components/Menu.astro";
import Banner from "../components/Banner.astro";
import AboutIWD from "../components/AboutIWD.astro";
import WTMProgram from "../components/WTMProgram.astro";
import GDGProgram from "../components/GDGProgram.astro";
import Footer from "../components/Footer.astro";
import Sponsors from "../components/Sponsors.astro";
import WTMAmbassadors from "../components/WTMAmbassadors.astro";
---

<Layout title="IWD Cochabamba 2024">
  <main>
    <Menu />
    <Banner />
    <AboutIWD />
    <WTMProgram />
    <WTMAmbassadors/>
    <GDGProgram />
    <Sponsors/>
    <Footer />
  </main>
</Layout>

<style>
  @font-face {
    font-family: "Google Sans";
    src: url("../assets/fonts/GoogleSansDisplay-Regular.ttf") format("truetype");
  }

  body {
    font-family: "Google Sans", sans-serif;
  }
</style>
